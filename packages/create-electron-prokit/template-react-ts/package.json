{
  "name": "{{name}}",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/dev.ts",
