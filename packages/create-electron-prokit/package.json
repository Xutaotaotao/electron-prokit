{
  "name": "create-electron-prokit",
  "version": "2.0.0",
  "engines": {
    "node": "^14.8.0 || >=16.0.0"
  },
  "description": "A cli to create a electron prokit project",
  "publishConfig": {
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "src/index.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "bin": {
    "create-electron-prokit": "dist/index.js"
  },
  "scripts": {
    "dev": "node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "start": "node --experimental-specifier-resolution=node dist/index.js"
  },
  "keywords": [
    "Electron",
    "electron",
    "electron-prokit",
    "electron prokit",
    "Electron Prokit",
    "Prokit",
    "prokit",
    "create-electron-prokit"
  ],
  "author": "Xutaotaotao",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.5.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/git-clone": "^0.2.1",
    "@types/inquirer": "^9.0.3",
    "fs-extra": "^11.1.1",
    "git-clone": "^0.2.0",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.11",
    "ora": "^7.0.1"
  }
}
