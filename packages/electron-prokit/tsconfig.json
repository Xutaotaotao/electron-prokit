{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "allowJs": true,
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "downlevelIteration": true,
    "declaration": true,
    "esModuleInterop": true
  },
  "types": [
    "jest"
  ],
  "include": ["src","test","vendor.d.ts","typings.d.ts"],
  "exclude": ["node_modules", "dist/**/*"],
}
