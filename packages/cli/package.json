{
  "name": "@electron-prokit/cli",
  "version": "0.0.0",
  "description": "A cli to create a electron project",
  "publishConfig": {
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "bin": {
    "electron-prokit-cli": "dist/index.js"
  },
  "scripts": {
    "dev": "node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "start": "node --experimental-specifier-resolution=node lib/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.5.7",
    "commander": "^11.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/git-clone": "^0.2.1",
    "@types/inquirer": "^9.0.3",
    "fs-extra": "^11.1.1",
    "git-clone": "^0.2.0",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.11",
    "ora": "^7.0.1"
  }
}
